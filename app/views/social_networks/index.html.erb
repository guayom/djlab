<p id="notice"><%= notice %></p>

<h1>Listing Social Networks</h1>

<table>
  <thead>
    <tr>
      <th>Network</th>
      <th>Url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @social_networks.each do |social_network| %>
      <tr>
        <td><%= social_network.network %></td>
        <td><%= social_network.url %></td>
        <td><%= link_to 'Show', social_network %></td>
        <td><%= link_to 'Edit', edit_social_network_path(social_network) %></td>
        <td><%= link_to 'Destroy', social_network, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Social network', new_social_network_path %>
